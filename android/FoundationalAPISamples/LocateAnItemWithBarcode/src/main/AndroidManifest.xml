<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@drawable/dbr_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/dbr_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoundationalAPISamples">
        <activity
            android:name=".LocateStartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".LocateGrabCodeActivity"
            android:exported="false"/>

        <activity android:name=".LocateScanActivity"
            android:exported="false"/>
    </application>

</manifest>